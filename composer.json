        "source": "https://github.com/ramsey/moontoast-math"
    },
    "require": {
        "php": ">=5.3.3",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.5 || ^6.5 || ^7.0"
    },
    "autoload": {
        "psr-4": {
